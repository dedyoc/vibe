    "default_filters": {
      "trend_date": {
        "operator": "TEMPORAL_RANGE",
        "value": "Last week"
      }
    },
    "filter_scopes": {},
    "chart_configuration": {
      "daily-trends": {
        "viz_type": "line",
        "datasource": "historical_trend_patterns",
        "x_axis_label": "Date",
        "y_axis_label": "Average Frequency",
        "groupby": ["keyword"],
        "metrics": ["avg_frequency"],
        "timeseries_limit_metric": "avg_frequency",
        "order_desc": true,
        "rolling_type": "mean",
        "rolling_periods": 3,
        "time_range": "Last 7 days",
        "granularity_sqla": "trend_date",
        "resample_rule": "1D",
        "resample_method": "mean"
      },
      "keyword-evolution": {
        "viz_type": "area",
        "datasource": "historical_trend_patterns",
        "x_axis_label": "Date",
        "y_axis_label": "Trend Occurrences",
        "groupby": ["keyword"],
        "metrics": ["trend_occurrences"],
        "order_desc": true,
        "contribution": false,
        "stacked_style": "stack",
        "time_range": "Last 30 days",
        "granularity_sqla": "trend_date"
      },
      "trend-distribution": {
        "viz_type": "pie",
        "datasource": "historical_trend_patterns",
        "groupby": ["keyword"],
        "metrics": ["trend_occurrences"],
        "row_limit": 10,
        "pie_label_type": "key_percent",
        "donut": true,
        "show_legend": true,
        "legend_position": "right"
      },
      "top-keywords-weekly": {
        "viz_type": "bar",
        "datasource": "historical_trend_patterns",
        "x_axis_label": "Keywords",
        "y_axis_label": "Max Growth Rate",
        "groupby": ["keyword"],
        "metrics": ["max_growth_rate"],
        "row_limit": 15,
        "order_desc": true,
        "rich_tooltip": true,
        "show_bar_value": true
      },
      "growth-patterns": {
        "viz_type": "box_plot",
        "datasource": "historical_trend_patterns",
        "groupby": ["keyword"],
        "metrics": ["max_growth_rate"],
        "whisker_options": "Tukey",
        "row_limit": 20,
        "order_desc": true
      }
    }
  },
  "css": ".dashboard-content { background-color: #f8f9fa; } .chart-container { border-radius: 8px; }",
  "owners": [1]
